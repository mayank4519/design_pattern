Circle	DrawShapes.hpp	/^class Circle : public Draw {$/;"	c
CircleFactory	DrawFactoryShapes.hpp	/^struct CircleFactory : Factory {$/;"	s
Draw	DrawShapes.hpp	/^class Draw {$/;"	c
DrawFactory	DrawFactory.hpp	/^    DrawFactory() {$/;"	f	class:DrawFactory
DrawFactory	DrawFactory.hpp	/^class DrawFactory {$/;"	c
Factory	DrawFactoryShapes.hpp	/^struct Factory {$/;"	s
Rectangle	DrawShapes.hpp	/^    Rectangle,$/;"	m	class:Shape
Rectangle	DrawShapes.hpp	/^class Rectangle : public Draw {$/;"	c
RectangleFactory	DrawFactoryShapes.hpp	/^struct RectangleFactory : Factory {$/;"	s
Shape	DrawShapes.hpp	/^enum class Shape {$/;"	c
Square	DrawShapes.hpp	/^    Square,$/;"	m	class:Shape
Square	DrawShapes.hpp	/^class Square : public Draw {$/;"	c
SquareFactory	DrawFactoryShapes.hpp	/^struct SquareFactory : Factory {$/;"	s
main	main.cpp	/^int main() {$/;"	f
make_pattern	DrawFactory.hpp	/^    std::unique_ptr<Draw> make_pattern(const Shape& s) {$/;"	f	class:DrawFactory
mp	DrawFactory.hpp	/^std::map<Shape, std::unique_ptr<Factory> > mp;$/;"	m	class:DrawFactory
